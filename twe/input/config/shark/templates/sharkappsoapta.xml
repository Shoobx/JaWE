<Application Id="soap" Name="SOAP">
   <FormalParameters>
      <FormalParameter Id="url" Name="URL to Web Service WSDL" IsArray="false" Mode="IN">
         <DataType>
            <BasicType Type="STRING"/>
         </DataType>
         <Description>String representing the URL to WSDL of the Web Service(e.g. http://localhost:8080/txw/TXWService?wsdl).</Description>
      </FormalParameter>
      <FormalParameter Id="operationName" Name="The name of Web Service operation" IsArray="false" Mode="IN">
         <DataType>
            <BasicType Type="STRING"/>
         </DataType>
         <Description>String representing the name of Web Service operation to execute.</Description>
      </FormalParameter>
   </FormalParameters>
   <ExtendedAttributes>
      <ExtendedAttribute Name="ToolAgentClass" Value="org.enhydra.shark.toolagent.SOAPToolAgent"/>
    </ExtendedAttributes>
</Application>
