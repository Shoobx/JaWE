<WorkflowProcess>
   <DataFields>
      <DataField Id="category" IsArray="false" Name="Outlook Category">
         <DataType>
            <BasicType Type="STRING"/>
         </DataType>
      </DataField>
      <DataField Id="i18n_lang_code" IsArray="false" Name="I18N language code">
         <DataType>
            <BasicType Type="STRING"/>
         </DataType>
         <Description>Representing language code for the process (e.g. en_US, de_AT, ja_JP_JP, th_TH_TH).</Description>
      </DataField>
   </DataFields>
   <ExtendedAttributes>
      <ExtendedAttribute Name="CHECK_FOR_FIRST_ACTIVITY" Value="false"/>
      <ExtendedAttribute Name="DYNAMIC_VARIABLE_HANDLING" Value="false"/>
      <ExtendedAttribute Name="CHECK_FOR_CONTINUATION" Value="false"/>
      <ExtendedAttribute Name="CHOOSE_NEXT_PERFORMER" Value="false"/>
      <ExtendedAttribute Name="ENABLE_REASSIGNMENT" Value="true"/>
   </ExtendedAttributes>
</WorkflowProcess>
