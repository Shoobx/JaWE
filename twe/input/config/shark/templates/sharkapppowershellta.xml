<Application Id="powershell" Name="PowerShell">
   <FormalParameters>
      <FormalParameter Id="result" Name="Result" IsArray="false" Mode="OUT">
         <DataType>
            <BasicType Type="STRING"/>
         </DataType>
         <Description>String representing output of PowerShell execution.</Description>
      </FormalParameter>
      <FormalParameter Id="error" Name="Error" IsArray="false" Mode="OUT">
         <DataType>
            <BasicType Type="STRING"/>
         </DataType>
         <Description>String representing error output of PowerShell execution.</Description>
      </FormalParameter>
   </FormalParameters>   
   <ExtendedAttributes>
      <ExtendedAttribute Name="ToolAgentClass" Value="org.enhydra.shark.toolagent.PowerShellToolAgent"/>
      <ExtendedAttribute Name="Script" Value=""/>
    </ExtendedAttributes>
</Application>
