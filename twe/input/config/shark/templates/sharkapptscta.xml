<Application Id="tsc" Name="TSC">
   <FormalParameters>
      <FormalParameter Id="source" Name="Calculation data source" IsArray="true" Mode="IN">
         <DataType>
            <ExternalReference location="byte"/>
         </DataType>
         <Description>byte array representing CSV or XLSX file for the calculation data source.</Description>
      </FormalParameter>
      <FormalParameter Id="calculation_output_template_filepath" Name="File path to calculation output template" IsArray="false" Mode="IN">
         <DataType>
            <BasicType Type="STRING"/>
         </DataType>
         <Description>String representing a file path to calculation output template (CSV or XLSX file).</Description>
      </FormalParameter>
      <FormalParameter Id="calculation_filepath" Name="File path to calculation" IsArray="false" Mode="IN">
         <DataType>
            <BasicType Type="STRING"/>
         </DataType>
         <Description>String representing a file path to calculation file (HTML file generated by Spreadsheet convertor).</Description>
      </FormalParameter>
      <FormalParameter Id="result" Name="Calculation data result" IsArray="true" Mode="OUT">
         <DataType>
            <ExternalReference location="byte"/>
         </DataType>
         <Description>byte array representing CSV or XLSX file for the calculation data result.</Description>
      </FormalParameter>
   </FormalParameters>
   <ExtendedAttributes>
      <ExtendedAttribute Name="ToolAgentClass" Value="org.enhydra.shark.toolagent.TSCToolAgent"/>
    </ExtendedAttributes>
</Application>
