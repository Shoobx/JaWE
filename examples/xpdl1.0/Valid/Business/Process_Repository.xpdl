                        <xpdl:ExtendedAttribute Name="VariableToProcess_VIEW" Value="Product_Name"/>
                        <xpdl:ExtendedAttribute Name="VariableToProcess_VIEW" Value="No_Of_Items_To_Produce"/>
                    </xpdl:ExtendedAttributes>
                </xpdl:Activity>
            </xpdl:Activities>
