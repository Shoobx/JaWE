                    <xpdl:Icon>document_delete.png</xpdl:Icon>
                    <xpdl:ExtendedAttributes>
                        <xpdl:ExtendedAttribute Name="VariableToProcess_VIEW" Value="Customer_Name"/>
                        <xpdl:ExtendedAttribute Name="VariableToProcess_VIEW" Value="Product_Name"/>
                        <xpdl:ExtendedAttribute Name="VariableToProcess_VIEW" Value="Order_Quantity"/>
                    </xpdl:ExtendedAttributes>
                    <xpdl:NodeGraphicsInfos>
                        <xpdl:NodeGraphicsInfo FillColor="187,247,190" Height="60" IsVisible="true" LaneId="Business_Example_pool1_lan1" ToolId="JaWE" Width="90">
                            <xpdl:Coordinates XCoordinate="512" YCoordinate="7"/>
                        </xpdl:NodeGraphicsInfo>
                    </xpdl:NodeGraphicsInfos>
                </xpdl:Activity>
                <xpdl:Activity FinishMode="Automatic" Id="Business_Example_Wor1_Act9" Name="R1" StartMode="Automatic">
                    <xpdl:Description>For partial shipments, check with customer and plan production in parallel</xpdl:Description>
                    <xpdl:Route GatewayType="Parallel"/>
                    <xpdl:TransitionRestrictions>
                        <xpdl:TransitionRestriction>
                            <xpdl:Split Type="Parallel">
                                <xpdl:TransitionRefs>
                                    <xpdl:TransitionRef Id="Business_Example_Wor1_Tra12"/>
                                    <xpdl:TransitionRef Id="Business_Example_Wor1_Tra13"/>
                                </xpdl:TransitionRefs>
                            </xpdl:Split>
                        </xpdl:TransitionRestriction>
                    </xpdl:TransitionRestrictions>
                    <xpdl:NodeGraphicsInfos>
                        <xpdl:NodeGraphicsInfo FillColor="255,229,124" Height="43" IsVisible="true" LaneId="Business_Example_pool1_lan2" ToolId="JaWE" Width="43">
                            <xpdl:Coordinates XCoordinate="427" YCoordinate="43"/>
                        </xpdl:NodeGraphicsInfo>
                    </xpdl:NodeGraphicsInfos>
                </xpdl:Activity>
                <xpdl:Activity FinishMode="Automatic" Id="Business_Example_Wor1_Act10" Name="check part. ship" StartMode="Automatic">
                    <xpdl:Description>Ask customer if OK to partial-ship (24 hrs)</xpdl:Description>
                    <xpdl:Implementation>
                        <xpdl:SubFlow Execution="SYNCHR" Id="Business_Example_Wor2">
                            <xpdl:ActualParameters>
                                <xpdl:ActualParameter>Customer_Name</xpdl:ActualParameter>
                                <xpdl:ActualParameter>Product_Name</xpdl:ActualParameter>
                                <xpdl:ActualParameter>Order_Quantity</xpdl:ActualParameter>
                                <xpdl:ActualParameter>No_Of_Stocked_Items</xpdl:ActualParameter>
                                <xpdl:ActualParameter>Partial_Ship_Status</xpdl:ActualParameter>
                            </xpdl:ActualParameters>
                        </xpdl:SubFlow>
                    </xpdl:Implementation>
                    <xpdl:Icon>mobilephone1.png</xpdl:Icon>
                    <xpdl:NodeGraphicsInfos>
                        <xpdl:NodeGraphicsInfo FillColor="255,106,106" Height="60" IsVisible="true" LaneId="Business_Example_pool1_lan2" ToolId="JaWE" Width="90">
                            <xpdl:Coordinates XCoordinate="538" YCoordinate="72"/>
                        </xpdl:NodeGraphicsInfo>
                    </xpdl:NodeGraphicsInfos>
                </xpdl:Activity>
                <xpdl:Activity FinishMode="Automatic" Id="Business_Example_Wor1_Act11" Name="plan part. production" StartMode="Automatic">
                    <xpdl:Description>Make plan assuming OK to partial-ship</xpdl:Description>
                    <xpdl:Implementation>
                        <xpdl:No/>
                    </xpdl:Implementation>
                    <xpdl:Performers>
                        <xpdl:Performer>Participant_Repository_Par12</xpdl:Performer>
                    </xpdl:Performers>
                    <xpdl:Icon>calculator.png</xpdl:Icon>
                    <xpdl:ExtendedAttributes>
                        <xpdl:ExtendedAttribute Name="VariableToProcess_UPDATE" Value="No_Of_Items_To_Produce"/>
                        <xpdl:ExtendedAttribute Name="VariableToProcess_VIEW" Value="Order_Quantity"/>
                        <xpdl:ExtendedAttribute Name="VariableToProcess_VIEW" Value="No_Of_Stocked_Items"/>
                    </xpdl:ExtendedAttributes>
                    <xpdl:NodeGraphicsInfos>
                        <xpdl:NodeGraphicsInfo FillColor="187,247,190" Height="60" IsVisible="true" LaneId="Business_Example_pool1_lan2" ToolId="JaWE" Width="90">
                            <xpdl:Coordinates XCoordinate="404" YCoordinate="117"/>
                        </xpdl:NodeGraphicsInfo>
                    </xpdl:NodeGraphicsInfos>
                </xpdl:Activity>
                <xpdl:Activity FinishMode="Automatic" Id="Business_Example_Wor1_Act13" Name="R2" StartMode="Automatic">
                    <xpdl:Description>For partial production, wait until check with customer</xpdl:Description>
                    <xpdl:Route GatewayType="Parallel"/>
                    <xpdl:TransitionRestrictions>
                        <xpdl:TransitionRestriction>
                            <xpdl:Join Type="Parallel"/>
                        </xpdl:TransitionRestriction>
                    </xpdl:TransitionRestrictions>
                    <xpdl:NodeGraphicsInfos>
                        <xpdl:NodeGraphicsInfo FillColor="255,229,124" Height="43" IsVisible="true" LaneId="Business_Example_pool1_lan2" ToolId="JaWE" Width="43">
                            <xpdl:Coordinates XCoordinate="561" YCoordinate="158"/>
                        </xpdl:NodeGraphicsInfo>
                    </xpdl:NodeGraphicsInfos>
                </xpdl:Activity>
                <xpdl:Activity FinishMode="Automatic" Id="Business_Example_Wor1_Act14" Name="plan full production" StartMode="Automatic">
                    <xpdl:Description>Either out-of-stock condition or rejection of partial ship can start full-order planning. Make plan to produce entire order</xpdl:Description>
                    <xpdl:Implementation>
                        <xpdl:No/>
                    </xpdl:Implementation>
                    <xpdl:Performers>
                        <xpdl:Performer>Participant_Repository_Par12</xpdl:Performer>
                    </xpdl:Performers>
                    <xpdl:TransitionRestrictions>
                        <xpdl:TransitionRestriction>
                            <xpdl:Join Type="Exclusive"/>
                        </xpdl:TransitionRestriction>
                    </xpdl:TransitionRestrictions>
                    <xpdl:ExtendedAttributes>
                        <xpdl:ExtendedAttribute Name="VariableToProcess_VIEW" Value="No_Of_Stocked_Items"/>
                        <xpdl:ExtendedAttribute Name="VariableToProcess_VIEW" Value="Order_Quantity"/>
                        <xpdl:ExtendedAttribute Name="VariableToProcess_UPDATE" Value="No_Of_Items_To_Produce"/>
                    </xpdl:ExtendedAttributes>
                    <xpdl:NodeGraphicsInfos>
                        <xpdl:NodeGraphicsInfo FillColor="187,247,190" Height="60" IsVisible="true" LaneId="Business_Example_pool1_lan2" ToolId="JaWE" Width="90">
                            <xpdl:Coordinates XCoordinate="729" YCoordinate="190"/>
                        </xpdl:NodeGraphicsInfo>
                    </xpdl:NodeGraphicsInfos>
                </xpdl:Activity>
                <xpdl:Activity FinishMode="Automatic" Id="Business_Example_Wor1_Act18" Name="produce" StartMode="Automatic">
