                    <xpdl:Icon>money.png</xpdl:Icon>
                    <xpdl:NodeGraphicsInfos>
                        <xpdl:NodeGraphicsInfo FillColor="255,106,106" Height="60" IsVisible="true" LaneId="Business_Example_pool1_lan1" ToolId="JaWE" Width="90">
                            <xpdl:Coordinates XCoordinate="315" YCoordinate="5"/>
                        </xpdl:NodeGraphicsInfo>
                    </xpdl:NodeGraphicsInfos>
                </xpdl:Activity>
                <xpdl:Activity FinishMode="Automatic" Id="Business_Example_Wor1_Act4" Name="check stock" StartMode="Automatic">
                    <xpdl:Description>Check the stock status of ordered product. Fill the variable Stock_Status with:
- "ALL" If there are enough items on stock,
- "PARTIAL" - if there are some stocked items (but not enough)
- "NONE" - if there are no any stocked item</xpdl:Description>
                    <xpdl:Implementation>
                        <xpdl:No/>
                    </xpdl:Implementation>
                    <xpdl:Performers>
                        <xpdl:Performer>Participant_Repository_Par11</xpdl:Performer>
                    </xpdl:Performers>
                    <xpdl:Icon>help.png</xpdl:Icon>
                    <xpdl:ExtendedAttributes>
                        <xpdl:ExtendedAttribute Name="VariableToProcess_UPDATE" Value="Stock_Status"/>
                        <xpdl:ExtendedAttribute Name="VariableToProcess_UPDATE" Value="No_Of_Stocked_Items"/>
                        <xpdl:ExtendedAttribute Name="VariableToProcess_VIEW" Value="Product_Name"/>
                        <xpdl:ExtendedAttribute Name="VariableToProcess_VIEW" Value="Order_Quantity"/>
                    </xpdl:ExtendedAttributes>
                    <xpdl:NodeGraphicsInfos>
                        <xpdl:NodeGraphicsInfo FillColor="187,247,190" Height="60" IsVisible="true" LaneId="Business_Example_pool1_lan1" ToolId="JaWE" Width="90">
                            <xpdl:Coordinates XCoordinate="403" YCoordinate="95"/>
                        </xpdl:NodeGraphicsInfo>
